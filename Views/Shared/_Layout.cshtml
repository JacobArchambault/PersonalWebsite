<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap-4.3.1-dist/css/bootstrap.css" />
    <link href="https://fonts.googleapis.com/css?family=Cormorant|Cormorant+Garamond|Cormorant+Infant|Cormorant+SC|Cormorant+Unicase|Cormorant+Upright&display=swap" rel="stylesheet">    
    <link href="~/lib/fontawesome-free-5.11.2-web/fontawesome-free-5.11.2-web/css/all.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    @await Html.PartialAsync("_Header")
    <main>
        @RenderBody()
    </main>
    @await Html.PartialAsync("_Footer")
    @await Html.PartialAsync("_Scripts")
</body>
</html>
